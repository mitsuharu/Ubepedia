{
  "name": "ubepedia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix dev",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "echo \"build command only checks typescript syntax and built-in lint.\" && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:project": "npx react-native-clean-project --keep-node-modules --remove-iOS-build --remove-iOS-pods --remove-android-build --clean-android-project --keep-brew --keep-pods",
    "clean:jest": "jest --clearCache",
    "test": "cross-env TZ=Asia/Tokyo jest",
    "lint": "eslint ./src/ --ext .js,.jsx,.ts,.tsx --quiet",
    "pod": "npx pod-install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.16",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "dayjs": "^1.10.7",
    "expo": ">=44.0.0-0 <45.0.0",
    "expo-asset": "^8.4.6",
    "expo-file-system": "^13.1.2",
    "expo-sqlite": "^10.1.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-swag-styles": "^0.2.2",
    "react-native-toast-message": "^2.1.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "typescript-fsa-redux-saga": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-jest": "^27.4.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-optional-require": "^0.3.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "jest-date-mock": "^1.0.8",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "jest-mock-extended": "^2.0.4",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
